{
  "name": "@zalter/identity",
  "version": "1.1.0",
  "author": "Zalter",
  "private": false,
  "description": "A Node.js API client for Zalter Identity service",
  "scripts": {
    "clean": "rimraf lib-cjs lib-esm",
    "build": "npm run clean && rollup --config rollup.config.js"
  },
  "repository": {
    "url": "https://github.com/zalter-com/zalter-identity-node.git"
  },
  "homepage": "https://zalter.com",
  "keywords": [
    "zalter",
    "akas",
    "sdk",
    "identity",
    "client",
    "authentication",
    "auth",
    "node",
    "js",
    "service",
    "api"
  ],
  "exports": {
    ".": {
      "types": "./lib-esm/index.d.ts",
      "import": "./lib-esm/index.mjs",
      "require": "./lib-cjs/index.js"
    }
  },
  "types": "./lib-esm/index.d.ts",
  "main": "./lib-cjs/index.js",
  "module": "./lib-esm/index.mjs",
  "files": [
    "lib-cjs",
    "lib-esm",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "engines": {
    "node": ">= 15.0.0"
  },
  "dependencies": {
    "@stablelib/cbor": "^1.0.1",
    "@stablelib/ed25519": "^1.0.2",
    "@zalter/http2-client-node": "^1.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.2.0",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/node": "^17.0.35",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3"
  }
}
